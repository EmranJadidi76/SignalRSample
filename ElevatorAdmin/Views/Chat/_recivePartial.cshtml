@model string
    <div class="col-lg-12 col-md-12">
        <div class="card-box widget-user">
            <div>
                <img src="~/@ViewBag.Profile" style="float:left;" class="img-responsive img-circle" alt="user">
                <div class="wid-u-info" style="float:left;margin-left:25px">
                    <h4 class="m-t-0 m-b-5">@ViewBag.User</h4>
                    <p class="text-muted m-b-5 font-13" style="font-size:22px!important">@Model</p>
                </div>
            </div>
        </div>
    </div>

